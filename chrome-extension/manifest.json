{
	"name": "Gulp Blacklist Marker",
	"version": "0.1.0",
	"description": "Marks blacklisted gulp modules when browsing NPM.",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"https://www.npmjs.com/*"
			],
			"js": ["base.js"]
		}
	],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
